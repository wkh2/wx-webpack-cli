@font-face {
  font-family: 'JDZhengHT';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.scroll__message {
  margin-top: 40rpx;
}
.scroll__message .swiper__com {
  display: block;
  height: 84rpx;
  position: relative;
  box-shadow: 0 0 10rpx 0 #E1E1E1;
  transition: box-shadow 200ms cubic-bezier(0.4, 0, 0.2, 1);
  background: #ffffff;
  border-radius: 1px;
  width: 710rpx;
  box-sizing: border-box;
  margin: 0 auto;
}
.scroll__message .swiper__com .swiper__item {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #231818;
}
